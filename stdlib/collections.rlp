(__load "stdlib/numeric.rlp")
(__load "stdlib/defines.rlp")

(def (std/coll/empty) '())

(def (std/coll/map func values)
    (def length (length values))
    (def result '())
    (def i 0)
    (while (less-than i length)
        (push! result
               (func (nth values i)))
        (set! i (inc i)))
    result)

(def (std/coll/range n)
    (def i 0)
    (def res '())
    (while (less-than i n)
        (push! res i)
        (set! i (inc i)))
    res)

