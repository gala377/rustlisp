(def (std/coll/map func vals)
    (if (null? vals)
        vals
        (+ (list (func (head vals)))
           (std/coll/map func (tail vals)))))

(def (std/coll/range n) (std/coll/__range 0 n))

(def (std/coll/__range curr n)
    (if (eq? curr n)
        '()
        (+ (list curr) (std/coll/__range (+ curr 1) n))))
