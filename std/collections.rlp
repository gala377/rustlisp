(def (empty) '())

(def (map func values)
    (def length (length values))
    (def result '())
    (def i 0)
    (while (less-than i length)
        (push! result
               (func (nth values i)))
        (set! i (inc i)))
    result)

(def (range n)
    (def i 0)
    (def res '())
    (while (less-than i n)
        (push! res i)
        (set! i (inc i)))
    res)

(export empty map range)

