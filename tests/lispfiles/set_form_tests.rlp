
(def set_test "hello")
(assert-equal set_test "hello")
(set! set_test "not hello anymore")
(assert-equal set_test "not hello anymore")

(def list_set '(1 2 3 4 5 6))
(assert-equal list_set '(1 2 3 4 5 6))
(set! (list_set 3) "Hello?")
(assert-equal list_set '(1 2 3 "Hello?" 5 6))
(def (set-in-func)
  (assert-equal list_set '(1 2 3 "Hello?" 5 6))
  (def list_set '(11 12 13 14 15))
  (assert-equal list_set '(11 12 13 14 15))
  (set! (list_set 2) "works?")
  (assert-equal list_set '(11 12 "works?" 14 15))
  (set! list_set "completely overwritten")
  (assert-equal list_set "completely overwritten"))
(set-in-func)
(assert-equal list_set '(1 2 3 "Hello?" 5 6))
